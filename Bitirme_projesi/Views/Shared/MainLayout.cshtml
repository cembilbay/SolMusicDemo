
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zxx">
<head>
    <title>SolMusic</title>
    <meta charset="UTF-8">
    <meta name="description" content="SolMusic HTML Template">
    <meta name="keywords" content="music, html">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

    

    <!-- Favicon -->
    <link href="~/solmusic-master/solmusic-master/img/favicon.ico" rel="shortcut icon" />

    <!-- Google font -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,300i,400,400i,500,500i,600,600i,700,700i&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">





    <!-- Stylesheets -->
    <link rel="stylesheet" href="~/solmusic-master/solmusic-master/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/solmusic-master/solmusic-master/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/solmusic-master/solmusic-master/css/owl.carousel.min.css" />
    <link rel="stylesheet" href="~/solmusic-master/solmusic-master/css/slicknav.min.css" />
    <link rel="stylesheet" href="~/solmusic-master/solmusic-master/css/responsive.css" />

    <!-- Main Stylesheets -->
    <link rel="stylesheet" href="~/solmusic-master/solmusic-master/css/style.css" />


    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header section -->
    @await Html.PartialAsync("MainHeaderPartial")
    <!-- Header section end -->
    @RenderBody()
    <!-- Footer section -->
    @await Html.PartialAsync("MainFooterPartial")
    <!-- Footer section end -->
    <!--====== Javascripts & Jquery ======-->
    <script src="~/solmusic-master/solmusic-master/js/jquery-3.2.1.min.js"></script>
    <script src="~/solmusic-master/solmusic-master/js/bootstrap.min.js"></script>
    <script src="~/solmusic-master/solmusic-master/js/jquery.slicknav.min.js"></script>
    <script src="~/solmusic-master/solmusic-master/js/owl.carousel.min.js"></script>
    <script src="~/solmusic-master/solmusic-master/js/mixitup.min.js"></script>
    <script src="~/solmusic-master/solmusic-master/js/main.js"></script>


</body>
</html>

