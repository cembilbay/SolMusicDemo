@model EntityLayer.Concrate.Contact
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div>
    <dl class="row">
        <dt class="col-sm-2">ID:</dt>
        <dd class="col-sm-10">@Model.ContactID</dd>

        <dt class="col-sm-2">İsim:</dt>
        <dd class="col-sm-10">@Model.ContactUserName</dd>

        <dt class="col-sm-2">Mail Adresi:</dt>
        <dd class="col-sm-10">@Model.ContactMail</dd>

        <dt class="col-sm-2">Konu:</dt>
        <dd class="col-sm-10">@Model.ContactSubject</dd>

        <dt class="col-sm-2">Mesaj:</dt>
        <dd class="col-sm-10">@Model.ContactMessage</dd>

        <dt class="col-sm-2">Tarih:</dt>
        <dd class="col-sm-10">@Model.ContactDate</dd>

        <dt class="col-sm-2">Status:</dt>
        <dd class="col-sm-10">@Model.ContactStatus</dd>
    </dl>
</div>


